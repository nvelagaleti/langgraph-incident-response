# Jira OAuth Configuration
# Copy this file to .env and fill in your values

# OAuth Client Configuration
JIRA_OAUTH_CLIENT_ID=IRpTkSYRqY2sPc2FxhHOJGpib4klKpXP
JIRA_OAUTH_CLIENT_SECRET=your_client_secret_here

# OAuth Redirect URI (default: localhost:8080)
JIRA_OAUTH_REDIRECT_URI=http://localhost:8080/callback

# Generated tokens (will be filled by the OAuth script)
JIRA_OAUTH_ACCESS_TOKEN=
JIRA_OAUTH_REFRESH_TOKEN=

# Jira Instance URL (optional, for reference)
JIRA_URL=https://your-domain.atlassian.net
